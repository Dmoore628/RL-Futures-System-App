{
  "name": "rl-futures-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:comprehensive": "jest --config jest.config.comprehensive.js",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:performance": "jest --testPathPattern=performance",
    "test:security": "jest --testPathPattern=security",
    "test:cross-browser": "jest --testPathPattern=crossBrowser",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:accessibility && npm run test:performance && npm run test:security && npm run test:cross-browser && npm run test:e2e",
    "test:quality": "npm run test:coverage && npm run lint && npm run format:check && npm run type-check",
    "analyze": "vite build --mode analyze",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "lighthouse:ci": "lhci autorun",
    "bundle:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "optimize:images": "node scripts/optimize-images.js",
    "build:optimized": "npm run optimize:images && npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "update-dev": "cd .. && node sync-docker-configs.js && docker-compose -f docker-compose.dev.yml down && docker-compose -f docker-compose.dev.yml up -d --build",
    "update-prod": "cd .. && node sync-docker-configs.js && docker build --no-cache -t rl-futures-frontend:latest -f frontend/Dockerfile frontend",
    "sync-docker": "cd .. && node sync-docker-configs.js"
  },
  "dependencies": {
    "@types/papaparse": "^5.3.16",
    "papaparse": "^5.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "6.20.0",
    "tslib": "2.6.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.1.0",
    "@storybook/addon-interactions": "7.1.0",
    "@storybook/addon-links": "7.1.0",
    "@storybook/blocks": "7.1.0",
    "@storybook/react": "7.1.0",
    "@storybook/react-vite": "7.1.0",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.3",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "4.0.3",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.6.1",
    "jest-extended": "^4.0.2",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "3.0.0",
    "storybook": "7.1.0",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite-bundle-analyzer": "^0.7.0",
    "lighthouse": "^11.6.0",
    "lighthouse-ci": "^0.4.0"
  }
}
